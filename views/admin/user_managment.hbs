<main id="main" class="main">
  <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile</th>
      <th scope="button">Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th>{{inc @index}}</th>
      <td>{{this.Rname}}</td>
      <td>{{this.Remail}}</td>
      <td>{{this.Rnumber}}</td>
      <td>
        {{#if this.status}}
        <a href="/admin/block/{{this._id}}" class="btn btn-primary">Block</a>
        {{else}}
        <a href="/admin/unblock/{{this._id}}" class="btn btn-danger">Unblock</a>
        {{/if}}
      </td>
    {{/each}}
    
  </tbody>
 </table>
</main>

