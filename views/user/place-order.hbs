    <script>
        function applyCoupon(coupon_id, total) {
            console.log("clicked")
            console.log(total)
            $.ajax({
                url: "/applyCouponOffer/" + coupon_id,
                method: "get",
                data: {

                    total: total

                },
                success: (coupon) => {
                    document.getElementById('Cname').value = coupon.CouponName;
                    document.getElementById('Cpercentage').value = coupon.OfferPercentage;
                    per = coupon.OfferPercentage
                    total = document.getElementById("total").value
                    limit = total * 40 / 100
                    offerprice = limit * per / 100
                    finalPrice = total - offerprice

                    document.getElementById("tottalAmount").innerHTML = Math.ceil(finalPrice)
                    document.getElementById("newTotal").value = Math.ceil(finalPrice)
                    document.getElementById("couponApplied").innerHTML="Coupon applied"
                    $('.modal').modal('hide')
                    $('body').removeClass('modal-open')
                    $('.modal-backdrop').remove()
                    
                    {{!-- console.log("oooooooooo")
                    console.log(coupon) --}}



                }
            })
        }
